{
    "provider": {
        "name": "ExampleProvider"
    },
    "consumer": {
        "name": "dummy-consumer"
    },
    "interactions": [
        {
            "description": "get hello world response",
            "request": {
                "method": "GET",
                "path": "/hello-world"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "id": 21341245,
                    "content": "test"
                },
                "matchingRules": {
                    "body": {
                        "$.id": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.content": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": ""
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.12"
        }
    }
}